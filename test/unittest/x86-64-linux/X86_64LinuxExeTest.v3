component X86_64LinuxExeTest {
	new() {
//TODO: 	TestTiers.register("jit:", jitPreRun);
	}
	def jitPreRun(f: Function) -> bool {
		if (WasmFunction.?(f)) {
			var wf = WasmFunction.!(f);
			if (wf.decl.code.target_code.spc_entry == Pointer.NULL) return Target.spcCompile(wf);
		}
		return true;
	}
}