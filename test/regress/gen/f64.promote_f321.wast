(module
  (func (export "m0") (param ) (result f64)
    (f64.promote_f32 (f32.const inf))
  )
  (func (export "m1") (param ) (result f64)
    (f64.promote_f32 (f32.const nan))
  )
  (func (export "m2") (param ) (result f64)
    (f64.promote_f32 (f32.const -0))
  )
  (func (export "m3") (param ) (result f64)
    (f64.promote_f32 (f32.const 0x1.800000p2))
  )
)
(assert_return (invoke "m0" ) (f64.const inf))
(assert_return (invoke "m1" ) (f64.const nan))
(assert_return (invoke "m2" ) (f64.const -0))
(assert_return (invoke "m3" ) (f64.const 0x1.8000000000000p2))
