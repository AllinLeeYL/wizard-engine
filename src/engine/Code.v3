// Representation of a function's code.
class Code(orig: Array<byte>) {
	var code: Array<byte> = orig;
	var ctl_xfer: Array<(u23, i23, u10, u23)>; // pc, Î”pc, valcount, popcount
	var num_locals: int;
	var abs_start: int;
}
