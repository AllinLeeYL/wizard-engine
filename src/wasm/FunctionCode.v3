// Representation of a function's code.
class FunctionCode(orig: Array<byte>) {
	var code: Array<byte> = orig;
	var ctl_summary: Array<(u23, i23, u10, u23)>; // pc, Î”pc, valcount, popcount
	var num_locals: int;
}
