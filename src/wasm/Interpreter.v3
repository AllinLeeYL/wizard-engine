// Interpreter for Wasm bytecode.
class Interpreter(instance: Instance) extends Executor {
	def val_stack = Stack<Value>.new();
	def call_stack = Stack<Frame>.new();
}
class Frame {
	var instance: Instance;
	var func: FunctionDecl;
	var current_sp: int;
	var current_pc: int;
	var ret_pc: int;
}
