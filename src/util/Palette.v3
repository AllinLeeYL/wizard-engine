// Color palette for uniform coloring of Trace/Monitor outputs
component Palette {
	// unhighlighted coloring code output
	def LNO		= "\x1b[38;5;244m"; 	// GRAY12   - line/byte number
	def FUNC        = "\x1B[0;36m"; 	// CYAN	    - function name 

	// for highlighting
	def ACTIVE	= "\x1B[1;37m"; 	// WHITE    - for indicating selected/active
	def INACTIVE	= "\x1b[38;5;244m"; 	// GRAY12   - for indicating dead/inactive
	def COUNT	= "\x1B[1;35m"; 	// MAGENTA  - any numeric value
	def HIGH        = "\x1B[1;31m"; 	// B RED    - for anything 'high', e.g. for hotness
	def MEDIUM      = "\x1B[1;33m"; 	// B YELLOW - for anything 'medium'
	def LOW		= "\x1B[0;34m"; 	// BLUE	    -for anything 'low'
}
